#largest of three number
a=int(input("enter a number"))
b=int(input("enter a number"))
c=int(input("enter a number"))
if b<a>c:
   print("the largest number is",a)
elif a<b>c:
     print("the largest number is",b)
elif a<c>b:
     print("the largest number is",c)
